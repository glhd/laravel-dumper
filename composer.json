{
    "name": "glhd/laravel-dumper",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Chris Morrell",
            "homepage": "http://www.cmorrell.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10|^11|^12",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.47|^7.55|^8.36|^9.15|^10.8|11.x-dev|dev-master|dev-main",
        "friendsofphp/php-cs-fixer": "^3.9",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9|^10.5|^11.5"
    },
    "suggest": {
        "jdorn/sql-formatter": "Allows better SQL formatting and highlighting"
    },
    "autoload": {
        "psr-4": {
            "Glhd\\LaravelDumper\\": "src/"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Glhd\\LaravelDumper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "write-diffs": "WRITE_DIFFS=1 vendor/bin/phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Glhd\\LaravelDumper\\LaravelDumperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
